#!/usr/bin/env bash

set -euo pipefail

REPO_URL="https://api.github.com/repos/microsoft/vscode-js-debug/releases"
function sort_versions() {
  sed 'h; s/[+-]/./g; s/.p\([[:digit:]]\)/.z\1/; s/$/.z/; G; s/\n/ /' | \
    LC_ALL=C sort -t. -k 1,1 -k 2,2n -k 3,3n -k 4,4n -k 5,5n | awk '{print $2}'
}

echo $(eval curl -s $REPO_URL | grep '"tag_name"' | sed -E 's/.*"tag_name": "v?([^"]+)".*/\1/' | sort_versions)
